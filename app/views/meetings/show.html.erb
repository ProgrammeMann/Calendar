<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @meeting.name %>
</p>

<p>
  <strong>Start time:</strong>
  <%= @meeting.start_time %>
</p>

<p>
  <strong>Desc:</strong>
  <%= @meeting.description %>
</p>
<%	if @meeting.user_id == current_user.id %>
	<%= link_to 'Edit', edit_meeting_path(@meeting) %> |
	<%= link_to 'Delete', @meeting, method: :delete, data: { confirm: 'Are you sure?' }  %> |
<% end %>
<%= link_to 'Back', meetings_path %> 
